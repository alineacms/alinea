{
  "bin": "./dist/cli.js",
  "name": "alinea",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "esbx build && run-p dev:compile dev:check dev:web2",
    "build": "esbx build",
    "alinea": "rimraf -g 'dist/**/*.js' && esbx build && node dist/cli/bin.js",
    "build:alinea": "esbx build",
    "release:types": "tsc",
    "release:build": "esbx build",
    "type": "tsc --diagnostics",
    "test": "esbx build && cross-env FORCE_COLOR=1 esbx test",
    "dev:check": "tsc -w",
    "dev:next": "yarn workspace @alinea/web dev",
    "dev:web2": "yarn workspace @alinea/web2 dev",
    "dev:compile": "esbx dev",
    "dev:stories": "ladle serve --stories \"src/**/*.stories.tsx\"",
    "gen:bundleTs": "esbx bundleTs",
    "build:stories": "ladle build --base /stories --stories  \"packages/**/*.stories.tsx\" --outDir apps/web/public/stories",
    "trace": "tsc --noEmit --incremental false --emitDeclarationOnly false --diagnostics --generateTrace private/trace && analyze-trace private/trace & speedscope private/trace/trace.json",
    "clean": "esbx clean",
    "bump": "esbx version",
    "prepare": "yarn build"
  },
  "exports": {
    ".": "./dist/index.js",
    "./css": "./dist/index.css",
    "./*.cjs": "./dist/*.cjs",
    "./*": "./dist/*.js"
  },
  "sideEffects": false,
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "workspaces": [
    "apps/demo",
    "apps/web",
    "apps/web2",
    "src/backend",
    "src/cli",
    "src/core",
    "src/dashboard",
    "src/store",
    "src/ui",
    "src/dev",
    "src/auth/passwordless",
    "src/input/richtext"
  ],
  "dependencies": {
    "@alinea/iso": "^0.1.21",
    "@alinea/sqlite-wasm": "^0.1.11"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@esbx/alias": "^0.0.20",
    "@esbx/extension": "^0.0.19",
    "@esbx/external": "^0.0.19",
    "@esbx/reload": "^0.0.19",
    "@esbx/reporter": "^0.0.19",
    "@esbx/run": "^0.0.19",
    "@esbx/sass": "^0.0.19",
    "@esbx/static": "^0.0.19",
    "@esbx/target": "^0.0.20",
    "@esbx/workspaces": "^0.0.19",
    "@ladle/react": "^2.0.2",
    "@types/glob": "^7.1.4",
    "@types/node": "^16.9.0",
    "@typescript/analyze-trace": "^0.9.1",
    "alinea": "0.0.0",
    "autoprefixer": "^10.4.2",
    "compare-versions": "^4.1.3",
    "copy-env-cli": "1.0.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.17.10",
    "esbx": "^0.0.18",
    "fs-extra": "^10.0.0",
    "glob": "^7.2.0",
    "memfs": "^3.4.1",
    "npm-run-all": "^4.1.5",
    "postcss-pxtorem": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "speedscope": "^1.14.0",
    "typescript": "^5.0.4",
    "typescript-plugin-css-modules": "^3.4.0",
    "uvu": "^0.5.1"
  },
  "packageManager": "yarn@3.2.0",
  "resolutions": {
    "esbuild": "0.17.10",
    "y-prosemirror": "1.1.2",
    "mime-db": "1.51.0",
    "prosemirror-model": "1.18.1",
    "prosemirror-schema-list": "1.2.0",
    "prosemirror-state": "1.4.1",
    "prosemirror-transform": "1.6.0",
    "prosemirror-view": "1.26.2"
  }
}
