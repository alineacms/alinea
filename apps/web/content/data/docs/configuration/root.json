{
  "id": "28hcdyq0sGJdgY6kM24NZd1XP5S",
  "type": "Doc",
  "root": "data",
  "index": "ZwG",
  "title": "Root",
  "blocks": [
    {
      "id": "28hceGrb2J5s9SSlpfoYOOgR3KC",
      "index": "a0",
      "type": "TextBlock",
      "text": [
        {
          "type": "heading",
          "level": 1,
          "textAlign": "left",
          "content": [
            {
              "type": "text",
              "text": "Root"
            }
          ]
        },
        {
          "type": "paragraph",
          "textAlign": "left",
          "content": [
            {
              "type": "text",
              "text": "Roots provide the means to organize content hierarchically in a tree. Pages can have subpages, subpages can have sub-subpages and so on. The hierarchy starts at a `root`."
            }
          ]
        },
        {
          "id": "28hkhm5hPbTobb7YrlwX7qCuNJE",
          "type": "CodeBlock",
          "code": "workspace('My root', {\n  icon, contains, i18n\n})"
        },
        {
          "type": "heading",
          "level": 2,
          "textAlign": "left",
          "content": [
            {
              "type": "text",
              "text": "Configuration"
            }
          ]
        },
        {
          "type": "heading",
          "level": 3,
          "textAlign": "left",
          "content": [
            {
              "type": "text",
              "text": "Icon"
            }
          ]
        },
        {
          "type": "paragraph",
          "textAlign": "left",
          "content": [
            {
              "type": "text",
              "text": "An icon can be used to label a root in the sidebar. Must be a react component. You can find icons on https://icones.js.org or install a package such as react-icons (https://react-icons.github.io/react-icons)."
            }
          ]
        },
        {
          "type": "heading",
          "level": 3,
          "textAlign": "left",
          "content": [
            {
              "type": "text",
              "text": "Contains"
            }
          ]
        },
        {
          "type": "paragraph",
          "textAlign": "left",
          "content": [
            {
              "type": "text",
              "text": "An array of strings containing the names of types that can be created at the root."
            }
          ]
        },
        {
          "id": "28hlDtsmVPBtekQz2fiKZiQ0l7R",
          "type": "CodeBlock",
          "code": "// only entries of type Page can be created at the top of the tree\ncontains: ['Page']"
        },
        {
          "type": "heading",
          "level": 3,
          "textAlign": "left",
          "content": [
            {
              "type": "text",
              "text": "I18n"
            }
          ]
        },
        {
          "type": "paragraph",
          "textAlign": "left",
          "content": [
            {
              "type": "text",
              "text": "An object with internationalization configuration. Locales is an array of strings, containing locales described by a key that will show up in the url structure as well."
            }
          ]
        },
        {
          "id": "28hlSdI7ooykir1ldfHW4lLtIUL",
          "type": "CodeBlock",
          "code": "i18: {\n  locales: ['en', 'fr', 'de']\n}"
        }
      ]
    },
    {
      "id": "28hmVtv8Rq0spMaqfLD8BrJBwIM",
      "index": "a1",
      "type": "TypesBlock",
      "types": "RootOptions"
    }
  ]
}