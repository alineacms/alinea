{
  "private": true,
  "name": "@alinea/demo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "wireit",
    "generate": "wireit",
    "build": "wireit",
    "start": "wireit"
  },
  "wireit": {
    "dev": {
      "command": "alinea serve -- next dev",
      "dependencies": [
        "../..:build"
      ]
    },
    "generate": {
      "command": "alinea generate",
      "dependencies": [
        "../..:build"
      ]
    },
    "build": {
      "command": "next build",
      "dependencies": [
        "generate"
      ]
    },
    "start": {
      "command": "next start",
      "dependencies": [
        "build"
      ]
    }
  },
  "dependencies": {
    "@alinea/content": "link:.alinea",
    "alinea": "0.0.0",
    "include-media": "^1.4.10",
    "next": "^12.2.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-frame-component": "^5.2.3"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.3.1",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.0",
    "wireit": "^0.7.1"
  }
}
